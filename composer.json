{

    "name": "vipkwd/utils",
    "description": "PHP常用函数集合",
    "type": "library",
    "version":"1.1.0",
    "homepage": "https://www.vipkwd.com",
    "authors": [
        {
            "name": "Vipkwd",
            "email": "service@vipkwd.com"
        }
    ],
    "keywords": ["vipkwd", "utils","php"],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "awesomite/var-dumper": "dev-master",
        "lys/php-shard-upload": "^4.4",
        "phpmailer/phpmailer": "6.1.6",
        "phpoffice/phpspreadsheet": "*",
        "symfony/console": "^5.3",
        "zoujingli/ip2region": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Vipkwd\\Utils\\": ["src/","support/libs/"]
        },
        "files": [
            "support/VipkwdException.php",
            "support/VipkwdPreload.php"
        ]
    },
    "minimum-stability": "dev",
    "scripts": {
        "post-install-cmd": [
            "chmod +x artisan",
            "@php -r \"file_exists('./index.php') || copy('index.php', 'index.php1');\""
        ],
        "post-root-package-install": [],
        "post-autoload-dump": [],
        "post-package-install": [],
        "post-package-update": []
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
