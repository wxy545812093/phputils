{

    "name": "vipkwd/utils",
    "description": "PHP常用函数集合",
    "type": "library",
    "homepage": "https://www.vipkwd.com",
    "authors": [
        {
            "name": "Vipkwd",
            "email": "service@vipkwd.com"
        }
    ],
    "keywords": ["vipkwd", "utils","php"],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "phpmailer/phpmailer": "6.1.6",
        "phpoffice/phpspreadsheet": "*",
        "symfony/console": "^5.3",
        "php-amqplib/php-amqplib":"2.10.*",
        "lys/php-shard-upload": "^4.4"
    },
    "require-dev": { },
    "autoload": {
        "psr-4": {
            "Vipkwd\\Utils\\": ["src/"]
        },
        "files": [
            "files/VipkwdException.php"
        ]
    },
    "minimum-stability": "dev",
    "scripts": {
      "post-install-cmd": [
        "chmod +x artisan"
      ],
      "post-root-package-install": [
        "@php -r \"file_exists('vendor/vipkwd/utils/artisan') || copy('vendor/vipkwd/utils/artisan', 'vipkwdArtisan');\""
      ]
    }
}
